{
    "name": "govcms/config",
    "description": "Configuration for the govCMS Drupal distribution",
    "keywords": ["govCMS"],
    "type": "project",
    "license": "GPL-2.0+",
    "require": {
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "^1.6.0",
        "wikimedia/composer-merge-plugin": "^1.4.0"
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "packagist.drupal-composer.org": {
            "discover-make-files": false
        },
        "enable-patching": true,
        "merge-plugin": {
            "require": [
                "composer.required.json"
            ],
            "include": [],
            "recurse": true,
            "merge-dev": true,
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true,
            "replace": true
        }
    },
    "conflict": {
        "drupal/core": "7.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
