{
    "name": "govcms/config",
    "description": "Configuration for the govCMS Drupal distribution",
    "keywords": ["govCMS"],
    "type": "project",
    "license": "GPL-2.0+",
    "require": {
        "composer/composer": "^1.4",
        "composer/installers": "^1.2.0",
        "composer-plugin-api": "^1.1",
        "cweagans/composer-patches": "^1.6.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "phing/phing": "^2.15.0",
        "php": ">=5.6",
        "phpunit/phpunit": "^6.1",
        "wikimedia/composer-merge-plugin": "^1.4.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "govCMS\\Config\\": "src/"
        }
    },
    "extra": {
        "class": "govCMS\\Config\\Composer\\Plugin"
    },
    "conflict": {
        "drupal/core": "7.*"
    },
    "suggest": {
        "hirak/prestissimo": "^0.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
